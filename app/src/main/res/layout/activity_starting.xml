<?xml version="1.0" encoding="utf-8"?>
<RelativeLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.omriakerman.goigahf2.StartingActivity">

    <Button
        android:id="@+id/goToStudent_button"
        android:layout_width="120dp"
        android:layout_height="53dp"
        android:elevation="2dp"
        android:onClick="startStudent"
        android:text="I'm a student"
        tools:layout_editor_absoluteX="228dp"
        tools:layout_editor_absoluteY="16dp"
        android:layout_marginRight="55dp"
        android:layout_marginEnd="55dp"
        android:layout_alignParentTop="true"
        android:layout_alignParentRight="true"
        android:layout_alignParentEnd="true"
        android:layout_marginTop="187dp" />

    <Button
        android:id="@+id/goToInstructor_button"
        android:layout_width="120dp"
        android:layout_height="53dp"
        android:text="I'm an instructor"
        tools:layout_editor_absoluteX="36dp"
        tools:layout_editor_absoluteY="16dp"
        android:onClick="startInstructor"
        android:layout_marginRight="25dp"
        android:layout_marginEnd="25dp"
        android:layout_alignBaseline="@+id/goToStudent_button"
        android:layout_alignBottom="@+id/goToStudent_button"
        android:layout_toLeftOf="@+id/goToStudent_button"
        android:layout_toStartOf="@+id/goToStudent_button" />

</RelativeLayout>
